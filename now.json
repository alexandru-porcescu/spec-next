{
  "version": 2,
  "github": {
    "silent": true
  },
  "env": {
    "SIMPLECAST_API_KEY": "@simplecast-api-key"
  },
  "build": {
    "env": {
      "SIMPLECAST_API_KEY": "@simplecast-api-key"
    }
  },
  "routes": [
    {
      "src": "/api/*",
      "headers": {
        "Cache-Control": "max-age=3600"
      },
      "continue": true
    }
  ]
}